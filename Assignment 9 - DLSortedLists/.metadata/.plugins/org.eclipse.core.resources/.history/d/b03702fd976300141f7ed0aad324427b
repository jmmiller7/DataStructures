package lists;

public class ItemNode<T>{
	private T info;
	private ItemNode<T> forward, back;
	
	public ItemNode<T> getBack() {
		return back;
	}

	public void setBack(ItemNode<T> back) {
		this.back = back;
	}

	public ItemNode(T item){
		info  = item;
		forward = null;
		back = null;
	}

	

	public T getInfo() {
		return info;
	}

	public void setInfo(T info) {
		this.info = info;
	}

	public ItemNode<T> getLink() {
		return forward;
	}

	public void setLink(ItemNode<T> link) {
		this.forward = link;
	}
}